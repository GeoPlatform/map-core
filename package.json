    "module": "dist/esm2015/index.js",
    "typings": "dist/index.d.ts",
    "version": "0.4.0",
    "license": "MIT",
    "ngPackage": {
        "lib": {
            "entryFile": "src/index.ts",
            "externals": [
                "q",
                "jquery",
                "leaflet",
                "esri-leaflet",
                "leaflet-draw",
                "leaflet-timedimension/dist/leaflet.timedimension.min",
                "geoplatform.client",
                "@geoplatform/client"
            ],
            "umdModuleIds": {
                "q": "Q",
                "jquery": "jQuery",
                "esri-leaflet": "L.esri",
                "leaflet": "L",
                "leaflet-draw": "L.Draw",
                "leaflet-timedimension/dist/leaflet.timedimension.min": "L.TimeDimension",
                "geoplatform.client": "GeoPlatformClient",
                "@geoplatform/client": "geoplatform.client",
                "geoplatform": "geoplatform"
            },
            "globals": {
                "q": "Q",
                "jQuery": "jQuery",
                "esri-leaflet": "L.esri",
                "leaflet": "L",
                "leaflet-draw": "L.Draw",
                "geoplatform.client": "GeoPlatformClient",
                "geoplatform": "GeoPlatform"
            }
        },
        "name": "MapCore",
        "moduleName": "MapCore",
        "amdId": "MapCore",
        "dest": "dist"
    },
    "whitelistedNonPeerDependencies": [
        "."
    ],
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "build": "ng-packagr -p package.json",
        "postbuild": "gulp && mkdir dist/css && cp -R src/**/*.css dist/css/",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e"
    },
    "pre-commit": [
        "build"
    ],
    "private": true,
    "peerDependencies": {
